	journal = {Software Quality Journal},
	journaltitle = {Software Quality Journal},
	number = {3},
	pages = {265--283},
	title = {Requirements Engineering and Process Modelling in Software Quality Management---Towards a Generic Process Metamodel},
	url = {https://doi.org/10.1023/B:SQJO.0000034711.87241.f0},
	volume = {12},
	year = {2004},
